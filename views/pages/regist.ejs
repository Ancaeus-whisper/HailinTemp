<!DOCTYPE html>
<html>

<head>
    <title>注册界面</title>
    <link rel="stylesheet" type="text/css" href="/login1.css">
    <script>
        function timelate() {
            var username = document.getElementsByName("named").values;
            var useridentity = document.getElementsByName("identity").values;
            var userpassword = document.getElementsByName("password").values;
            var userispassword = document.getElementsByName("ispassword").values;
            var captcha = document.getElementsByName("captcha").values;
            var userphone = document.getElementsByName("phone").values;
            $.ajax({
                type: "post",
                url: "",
                data: {
                    "registName": username,
                    "registIdentity": useridentity,
                    "registPhone": userphone,
                    "registCaptcha": usercaptcha,
                    "registPwd": userpassword,
                    "registisPwd": userispassword
                },
                dataType: "json",
                async: false,
                success: function(data) {
                    alert("传输成功");
                },
                error: function() {
                    alert("传输失败");
                }
            });
        }
    </script>
</head>

<body>
    <form id="login" action="#" method="GET" name="reform"enctype="multipart/form-data" onsubmit="return timelate();">
        <div class="login">
            <!--<div style="background: url(images/bg.jpg);width: 600px;height: 500px;"></div>-->
            <h2>注册</h2>
            <div class="box">
                姓名：&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <input type="text" name="named" required="required" placeholder="请填写姓名">
            </div>
            <div class="box">
                身份证：&nbsp;&nbsp;&nbsp;
                <input type="password" name="identity" required="required" placeholder="请填写身份证">
            </div>
            <div class="box">
                密码：&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <input type="password" name="password" required="required" placeholder="请填写密码">
            </div>
            <div class="box">
                确认密码：
                <input type="password" name="ispassword" required="required" placeholder="请确认密码">
            </div>
            <div class="box">
                手机号：&nbsp;&nbsp;&nbsp;
                <input type="text" name="phone" required="required" placeholder="请填写手机号">
            </div>
            <div class="box1">
                验证码：&nbsp;&nbsp;&nbsp;
                <input type="text" name="captcha" required="required" placeholder="请填写验证码">
                <a href="javascript:void(0)" id="send-sms" class="send-sms" data-bind="text: send_sms, click: sendSMS, css: {disabled: send_sms() != str().send_sms || typeof phoneStatus() === 'string'}, hasHover: sendSMSHover" tabindex="6">发送短信验证码</a>
            </div>
            <div class="in">
                <a href="javascript: document.reform.submit() ">注册</a>
        </a>
            </div>
        </div>
    </form>
</body>

</html>